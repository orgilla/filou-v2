{
  "name": "filou-mono",
  "private": true,
  "license": "MIT",
  "keywords": [
    "react",
    "webworker",
    "db",
    "database",
    "offline-first",
    "nosql",
    "jsonschema",
    "rxjs",
    "pwa",
    "localstorage",
    "indexeddb",
    "encryption",
    "pouchdb",
    "couchdb",
    "rx",
    "reactive",
    "realtime"
  ],
  "author": "Orgilla <hi@orgil.la>",
  "bugs": {
    "url": "https://github.com/bkniffler/workerdb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bkniffler/workerdb.git"
  },
  "workspacesAll": [
    "packages/*",
    "typescript/*"
  ],
  "workspaces": [
    "typescript/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o storybook-out",
    "up": "yarn upgrade-interactive --latest",
    "netlify": "yarn build && cd example/react && yarn && yarn build",
    "docs": "docsify serve docs",
    "bootstrap": "lerna bootstrap",
    "build": "lerna run build",
    "clean": "lerna run --parallel clean",
    "clean:full": "rm -rf ./node_modules && rm -rf ./packages/*/node_modules && rm -rf ./packages/*/lib && rm -rf ./packages/*/es",
    "dev": "lerna run --parallel dev",
    "dev:es": "lerna run --parallel dev -- --module esnext --outDir es",
    "lp": "yarn build && yarn test && lerna publish --registry https://registry.npmjs.org",
    "lpf": "lerna publish --registry https://registry.npmjs.org",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "coverage": "codecov"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@storybook/addon-info": "^3.4.0",
    "@storybook/addon-knobs": "^3.4.0",
    "@storybook/addon-storyshots": "^3.4.11",
    "@storybook/addon-storyshots-puppeteer": "^4.0.0-alpha.11",
    "@storybook/react": "^4.0.0-alpha.23",
    "@types/jest": "^23.3.7",
    "@types/node": "^10.12.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/recompose": "^0.27.0",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__addon-storyshots": "^3.4.3",
    "@types/storybook__react": "^3.0.8",
    "babel-core": "^6.26.0",
    "babel-loader": "^8.0.4",
    "codecov": "^3.1.0",
    "docsify-cli": "^4.2.1",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "npm-run-all": "^4.1.3",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-test-renderer": "^16.6.0",
    "regenerator-runtime": "^0.12.1",
    "storybook-addon-jsx": "^5.3.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.3"
  },
  "babelDependencies": {
    "@babel/cli": "7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.1.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-import-7": "^1.6.7",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-imports": "^1.5.1"
  },
  "resolve": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "typescript/*/src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "html",
      "cobertura",
      "text"
    ]
  }
}
