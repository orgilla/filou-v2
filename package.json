{
  "name": "filou-mono",
  "private": true,
  "license": "MIT",
  "keywords": [
    "react",
    "webworker",
    "db",
    "database",
    "offline-first",
    "nosql",
    "jsonschema",
    "rxjs",
    "pwa",
    "localstorage",
    "indexeddb",
    "encryption",
    "pouchdb",
    "couchdb",
    "rx",
    "reactive",
    "realtime"
  ],
  "author": "Orgilla <hi@orgil.la>",
  "bugs": {
    "url": "https://github.com/bkniffler/workerdb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bkniffler/workerdb.git"
  },
  "workspacesAll": [
    "packages/*",
    "typescript/*"
  ],
  "workspaces": [
    "typescript/*"
  ],
  "scripts": {
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c .storybook -o storybook-out",
    "up": "yarn upgrade-interactive --latest",
    "netlify": "yarn build && cd example/react && yarn && yarn build",
    "bootstrap": "lerna bootstrap",
    "watch": "lerna exec --parallel -- node ../../node_modules/typescript/bin/tsc -w -p ./tsconfig.json",
    "build": "run-p build:*",
    "build:ts": "lerna exec --parallel -- node ../../node_modules/typescript/bin/tsc -p ./tsconfig.json",
    "build:bundle": "lerna exec --parallel -- node ../../node_modules/rollup/bin/rollup -c ../../rollup.config.js",
    "clean": "run-p clean:*",
    "clean:ts": "lerna exec --parallel -- node ../../node_modules/rimraf/bin.js {__tests__,___tests__,stories,src}/**/*.{d.ts,map,js}",
    "clean:lib": "lerna exec --parallel -- node ../../node_modules/rimraf/bin.js lib",
    "clean:config": "lerna exec --parallel -- node ../../node_modules/rimraf/bin.js config",
    "lp": "yarn build && yarn test && lerna publish --registry https://registry.npmjs.org",
    "lpf": "lerna publish --registry https://registry.npmjs.org",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "coverage": "codecov"
  },
  "resolutions": {
    "@types/history": "^4.7.2",
    "@types/react": "^16.4.18",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "dependencies": {
    "@storybook/addon-info": "^4.0.2",
    "@storybook/addon-knobs": "^4.0.2",
    "@storybook/addon-storyshots": "^4.0.2",
    "@storybook/react": "^4.0.2",
    "@types/classnames": "^2.2.6",
    "@types/fs-extra": "^5.0.4",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.12.2",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/recompose": "^0.27.0",
    "@types/storybook__addon-info": "^3.2.3",
    "@types/storybook__addon-knobs": "^3.3.1",
    "@types/storybook__addon-storyshots": "^3.4.3",
    "@types/storybook__react": "^4.0.0",
    "codecov": "^3.1.0",
    "fela": "^6.2.3",
    "fela-dom": "^8.0.4",
    "ficon": "^0.2.15",
    "ficon-fela": "^0.2.15",
    "fs-extra": "^7.0.0",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.6.0",
    "react-docgen-typescript-loader": "^3.0.0",
    "react-dom": "^16.6.0",
    "react-fela": "^8.0.4",
    "react-test-renderer": "^16.6.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "storybook-addon-jsx": "^5.3.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^5.3.0",
    "ts-node": "^7.0.1",
    "typescript": "^3.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "typescript/*/src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "html",
      "cobertura",
      "text"
    ]
  }
}
