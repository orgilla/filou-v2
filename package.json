{
  "name": "filou-mono",
  "private": true,
  "license": "MIT",
  "keywords": [
    "react",
    "webworker",
    "db",
    "database",
    "offline-first",
    "nosql",
    "jsonschema",
    "rxjs",
    "pwa",
    "localstorage",
    "indexeddb",
    "encryption",
    "pouchdb",
    "couchdb",
    "rx",
    "reactive",
    "realtime"
  ],
  "author": "Orgilla <hi@orgil.la>",
  "bugs": {
    "url": "https://github.com/bkniffler/workerdb"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bkniffler/workerdb.git"
  },
  "workspacesAll": [
    "packages/*",
    "typescript/*"
  ],
  "workspaces": [
    "filou/*",
    "filou-blueprint/*"
  ],
  "scripts": {
    "docz": "docz dev",
    "netlify": "yarn bootstrap && docz build",
    "up": "yarn upgrade-interactive --latest",
    "bootstrap": "lerna bootstrap",
    "dev": "lerna run watch --stream --parallel",
    "build": "lerna run build",
    "clean": "run-p clean:*",
    "clean:ts": "lerna exec --parallel -- node ../../node_modules/rimraf/bin.js {__tests__,___tests__,stories,src}/**/*.{d.ts,map,js}",
    "clean:lib": "lerna exec --parallel -- node ../../node_modules/rimraf/bin.js lib",
    "lp": "yarn build && yarn test && lerna publish --registry https://registry.npmjs.org",
    "lpf": "lerna publish --registry https://registry.npmjs.org",
    "test": "jest",
    "test:update": "jest --updateSnapshot",
    "coverage": "codecov"
  },
  "resolutions": {
    "@types/history": "^4.7.2",
    "@types/react": "^16.4.18",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "dependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.5",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.1.0",
    "@types/classnames": "^2.2.6",
    "@types/fs-extra": "^5.0.4",
    "@types/history": "^4.7.2",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.118",
    "@types/minimist": "^1.2.0",
    "@types/node": "^10.12.3",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/react-test-renderer": "^16.0.3",
    "@types/recompose": "^0.27.0",
    "codecov": "^3.1.0",
    "docz": "^0.12.9",
    "docz-core": "^0.12.9",
    "fela": "^6.2.3",
    "fela-dom": "^8.0.4",
    "ficon": "^0.2.15",
    "ficon-fela": "^0.2.15",
    "fs-extra": "^7.0.0",
    "jest": "^23.6.0",
    "lerna": "^3.4.3",
    "lodash": "^4.17.11",
    "minimist": "^1.2.0",
    "npm-run-all": "^4.1.3",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-fela": "^8.0.4",
    "react-test-renderer": "^16.6.0",
    "regenerator-runtime": "^0.12.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.6"
  },
  "jest": {
    "preset": "ts-jest",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "typescript/*/src/**/*.{ts,tsx}"
    ],
    "coverageReporters": [
      "html",
      "cobertura",
      "text"
    ]
  }
}
