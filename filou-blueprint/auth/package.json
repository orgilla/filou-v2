    "@types/jwt-decode": "^2.2.1",
    "@types/validator": "^9.4.3",
    "axios": "^0.18.0",
    "emotion": "^10.0.2",
    "jwt-decode": "^2.2.0",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.67.4",
    "typescript": "^3.2.1"
  }
}
